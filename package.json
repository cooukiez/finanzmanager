{
  "name": "finanzmanager",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "dev:open": "vite dev --open",
    "dev:host": "vite dev --host",
    "dev:host:open": "vite dev --host --open",
    "build": "vite build",
    "preview": "vite preview",
    "preview:open": "vite preview --open",
    "preview:host": "vite preview --host",
    "preview:host:open": "vite preview --host --open",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "update": "npx npm-check-updates -u && npm install",
    "clean": "rm -rf build node_modules .svelte-kit package-lock.json",
    "reinstall": "npm run clean && npm install",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push",
    "prisma:studio": "prisma studio",
    "prisma:reset": "prisma migrate reset --force",
    "prisma:migrate": "prisma migrate dev --name init",
    "prisma:seed": "prisma db seed",
    "setup:deps": "npm install",
    "setup:prisma": "npm run prisma:generate && npm run prisma:migrate",
    "deploy:full": "npm run setup:deps && npm run setup:prisma && npm run dev:open",
    "deploy:quick": "npm run setup:prisma && npm run dev:open",
    "rsop": "npm run prisma:reset && npm run dev:open",
    "dvop": "npm run dev:open"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@layerstack/tailwind": "^0.0.11",
    "@lucia-auth/adapter-prisma": "^4.0.1",
    "@prisma/client": "^5.0.0",
    "@sveltejs/kit": "^2.0.0",
    "@tanstack/table-core": "^8.20.5",
    "cors": "^2.8.5",
    "d3-array": "^3.2.4",
    "d3-scale": "^4.0.2",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "formsnap": "^2.0.0",
    "layerchart": "^0.78.0",
    "lucia": "^3.2.2",
    "mode-watcher": "^0.5.1",
    "oslo": "^1.2.1",
    "svelte": "^5.0.0",
    "svelte-radix": "^2.0.1",
    "svelte-sonner": "^0.3.28",
    "sveltekit-superforms": "^2.23.1",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@types/cors": "^2.8.17",
    "@types/d3-array": "^3.2.1",
    "@types/d3-scale": "^4.0.9",
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.10",
    "autoprefixer": "^10.4.20",
    "bits-ui": "^1.3.4",
    "clsx": "^2.1.1",
    "lucide-svelte": "^0.475.0",
    "prettier": "^3.4.2",
    "prettier-plugin-svelte": "^3.3.3",
    "prisma": "^6.2.1",
    "svelte-check": "^4.0.0",
    "tailwind-merge": "^2.6.0",
    "tailwind-variants": "^0.3.1",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "unplugin-icons": "^22.0.0",
    "vite": "^5.4.11"
  }
}
